<?xml version="1.0" encoding="utf-8"?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd http://www.nlog-project.org/schemas/NLog.xsd" autoReload="true">
  <targets async="true">
    <target name="logfile" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\AgentLog.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="RESTHearbeat" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\RESTHeartbeat.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="RESTfile" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\RESTCommunications.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="warnfile" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\WarnLog.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="bcifile" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\ByteCode.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="btlog" xsi:type="File" archiveAboveSize="5000000" maxArchiveFiles="5" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\BusinessTransactionsLog.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="CoordinatorRecovery" xsi:type="File" archiveAboveSize="2000000" maxArchiveFiles="2" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\CoordinatorRecoveryLog.txt" layout="${longdate} ${processid} ${processname} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="Winston" xsi:type="File" archiveAboveSize="9000000" maxArchiveFiles="2" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\Winston.txt" layout="${longdate} ${processid} ${appdomainid} ${threadid} ${message}" />
    <target name="SamplingInfo" xsi:type="File" archiveAboveSize="9000000" maxArchiveFiles="2" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\SamplingInfo.txt" layout="${longdate} ${processid} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
    <target name="Analytics" xsi:type="File" archiveAboveSize="9000000" maxArchiveFiles="2" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\Analytics.txt" layout="${longdate} ${processid} ${appdomainid} ${threadid} ${message}" />
    <target name="Profiler" xsi:type="File" archiveAboveSize="12000000" maxArchiveFiles="2" createDirs="true" fileName="${environment:variable=PROGRAMDATA}\AppDynamics\DotNetAgent\Logs\Profiler" layout="${longdate} ${processid} ${appdomainid} ${threadid} ${level} ${logger:shortName=True} ${message}" />
  </targets>
  <rules>
    <logger name="com.appdynamics.REST.HeartBeatLog" minlevel="Info" writeTo="RESTHearbeat" final="true" />
    <logger name="*" minlevel="Warn" writeTo="warnfile" />
    <logger name="com.appdynamics.BusinessTransactions" minlevel="Info" writeTo="btlog" final="true" />
    <logger name="com.appdynamics.bci.*" minlevel="Info" writeTo="bcifile" final="true" />
    <logger name="com.appdynamics.REST.*" minlevel="Info" writeTo="RESTfile" final="true" />
    <logger name="AppDynamics.Agent.Coordinator.Recovery.*" minlevel="Info" writeTo="CoordinatorRecovery" final="true" />
    <logger name="com.appdynamics.METRICS.MetricSender" minlevel="Info" writeTo="RESTfile" final="true" />
    <logger name="AppDynamics.Winston.*" minlevel="Info" writeTo="Winston" final="true" />
    <logger name="com.appdynamics.tm.AFastBackendResolver" minlevel="Warn" writeTo="logfile" final="true" />
    <logger name="com.appdynamics.ManagedAgentAPI.DumpStats" minlevel="Trace" writeTo="SamplingInfo" final="true" />
    <logger name="com.appdynamics.ee.service.analytics.Analytics" minlevel="Info" writeTo="Analytics" final="true" />
    <logger name="com.appdynamics.profiler.*" minlevel="Info" writeTo="Profiler" />
    <logger name="*" minlevel="Info" writeTo="logfile" />
  </rules>
</nlog>
